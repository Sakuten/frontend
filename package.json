{
  "name": "frontend",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-mobx": "^1.0.3",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^2.2.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chokidar-cli": "^1.2.2",
    "css-loader": "0.28.7",
    "deep-assign": "^2.0.0",
    "doc-cookies": "^1.1.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.5.0",
    "enzyme-adapter-react-16": "1.3.0",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "23.0.0",
    "jest-localstorage-mock": "^2.2.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-router": "^4.0.4",
    "moment": "2.22.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "object-assign": "4.1.1",
    "polished": "2.0.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "querystring": "^0.2.0",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "16.4.2",
    "react-fontawesome": "^1.6.1",
    "react-qr-reader": "^2.1.0",
    "react-recaptcha-v3": "1.0.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "styled-components": "^3.3.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.10.0",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "snyk": "^1.161.1"
  },
  "scripts": {
    "build:css": "node-sass-chokidar --include-path src/style --include-path node_modules $ADDITIONAL_CSS_BUILD_OPTS src/style -o src/style",
    "start:css": "yarn run build:css && yarn run build:css --watch --recursive",
    "build:js": "node scripts/build.js",
    "start:js": "node scripts/start.js",
    "start": "npm-run-all -p start:deps start:css start:js",
    "build": "npm-run-all build:css build:js",
    "test": "node scripts/test.js --env=jsdom",
    "start:deps": "chokidar ./package.json -c 'yarn install --modules-folder /node_modules'",
    "lint": "eslint .",
    "fix": "yarn run lint --fix",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/api/**/*"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/setupTests.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "mobx"
    ]
  },
  "snyk": true
}
